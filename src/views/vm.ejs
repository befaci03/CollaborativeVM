<!DOCTYPE html>
<html lang="fr">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <script>/*ejs_injector_script*/window.vmConfig=<%-JSON.stringify(vmConfig,null,0)%>;window.wsprt=<%-wsVm%>;</script>

  <title><%= siteName %> # <%= vmId %></title>

  <script src="/socket.io/socket.io.js"></script>

  <script type="module" src="/js/CLIManager.js"></script>

  <link rel="stylesheet" href="/css/main.css">
  
  <script src="https://kit.fontawesome.com/bf76b26203.js" crossorigin="anonymous"></script>
  <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
  <script src="/js/storageManage.js" type="text/javascript"></script>
  <script src="/js/iconAnimations.js"></script>
</head>
<body>
  <nav>
    <font onclick="window.location.pathname = '/'" class="aSpan" style="font-size: 21px; font-weight: 750; margin-right: 18px;"><%= siteName %></font>
    <span class="aSpan" onclick="window.location.href = '/vms'">VM List</span>
    <span class="aSpan" id="themeToggler">null Mode</span>
    <% if (_config.links.discord && _config.links.discord !== "" && _config.links.discord !== "example" && _config.links.discord !== "home") { %>
      <span class="aSpan" onclick="window.location.href = '/externalLink?r=discord'"><i class="fa-brands fa-discord"></i> Discord</span>
    <% } %>
    <% if (_config.links.reddit && _config.links.reddit !== "" && _config.links.reddit !== "/r/example" && _config.links.reddit !== "/home") { %>
      <span class="aSpan" onclick="window.location.href = '/externalLink?r=reddit'"><i class="fa-brands fa-reddit fa-rotate-by" style="--fa-rotate-angle: -15deg;"></i> Reddit</span>
    <% } %>
    <% if (_config.links.twitter && _config.links.twitter !== "" && _config.links.twitter !== "example" && _config.links.twitter !== "home") { %>
      <span class="aSpan" onclick="window.location.href = '/externalLink?r=x&o=twitter'"><i class="fa-brands fa-x-twitter"></i> X</span>
    <% } %>
  </nav>
  <header>
    <center id="vmDetails">
      <h4>Click on the button to take a turn</h4>
      <font style="font-size: 17px; font-weight: bold;">VM: <%= vmId %></font>
      <div id="vmScreen"></div>
      <div id="vmcontrols">
        <ul class="liulLineContent">
          <li>
            <span class="spanBtn vmcontrol" id="cvmTaketurn">
              Start turn
            </span>
            <span class="spanBtn vmcontrol" id="vmKboard">
              Keyboard
            </span>
            <span class="spanBtn vmcontrol" id="vmKeyCAD">
              Ctrl+Alt+Del
            </span>
            <span class="spanBtn vmcontrol" id="voteReset">
              Start a reset vote
            </span>
          </li>
          <li>
            
          </li>
        </ul>
      </div>
    </center>

    <div id="column1" class="column list">
      Online users: <span id="onlineUsers_Counter"><%= socket.users.length %></span>
      <div class="list" id="userslist">
        <% socket.users.forEach(function(user) { %>
          <li><%= user.username %></li>
        <% }) %>
      </div>
    </div>
  </header>
</body>
</html>
